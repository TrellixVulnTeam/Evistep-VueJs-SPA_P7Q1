s\" type=\"password\" name=\"pass\" placeholder=\"Password\" id=\"password\">\n        <input class=\"input\" type=\"password\" name=\"repeatPass\" placeholder=\"Confirm Password\" id=\"confirmPassword\">\n        <i class=\"main-icons password-eye far fa-eye\" id=\"togglePassword\"></i>\n        <div class=\"inputRadioParent\">\n          <p><input type=\"radio\" name=\"checkAdmin\" class=\"inputRadio\" id=\"Admin\">Admin</p>\n          <p><input type=\"radio\" name=\"checkAdmin\" class=\"inputRadio\" id=\"User\">User</p>\n        </div>\n        <div id=\"example\" class=\"example1\">\n          <button id=\"btn-sign-in\" @click=\"collect\">Create an account</button>\n        </div>\n      </div>\n      <p  class=\"paragraph1\">\n        <b class=\"dark\">\n          or\n         <i class=\"login-btn\">\n            <router-link to=\"/Login\">login</router-link>\n         </i>\n          with\n        </b>\n      </p>\n    </div>\n  </div>\n</template>\n\n\n\n\n<script>\nimport Vue from \"vue\";\nexport default {\n  name: \"SignUpForm\"\n}\n\nnew Vue({\n  el: '#example',\n  methods: {\n    collect: function () {\n      alert(111);\n    }\n  }\n})\n\nwindow.onload = function() {\n  /*Shoe/Hide password*/\n  const togglePassword = document.querySelector('#togglePassword');\n  const password = document.querySelector('#password');\n\n  togglePassword.addEventListener('click', function () {\n    // toggle the type attribute\n    const type = password.getAttribute('type') === 'password' ? 'text' : 'password';\n    password.setAttribute('type', type);\n    // toggle the eye slash icon\n    this.classList.toggle('fa-eye-slash');\n  });\n}\n\n/*============================checking an email================================*/\n\n  /*/!* variables *!/\n  var btnClick = document.getElementById(\"btn-sign-in\");\n  var mailInput = document.getElementById(\"mail\");\n  var passInput = document.getElementById(\"password\");\n  var passCheck = document.getElementById(\"confirmPassword\");\n  var priorityInput = document.querySelector(\".inputRadio\");\n  var objectOfData = {\n    Email: \"\",\n    Password: \"\",\n    Priority: \"\"\n  };\n  var RegExpMail = /^\\w+[+.\\w-]*@([\\w-]+\\.)*\\w+[\\w-]*\\.([a-z]{2,4}|\\d+)$/i;\n  var letterNumber = /^[0-9a-zA-Z]+$/;\n\n/!*Main checking*!/\n  btnClick.addEventListener('click', function () {\n    if ((mailInput.match(RegExpMail)) && (passInput.match(letterNumber)) && (passInput === passCheck)) {\n      objectOfData.Email = mailInput;\n      objectOfData.Password = passInput;\n      objectOfData.priority = priorityInput;\n    }\n    else {\n      alert(\"Something went wrong, try again\");\n    }\n  });*/\n\n\n\n\n\n\n\n</script>\n\n\n\n\n<style scoped>\n/*form*/\n.example1{\n  width: 100%;\n  text-align: center;\n}\n.form{\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-wrap: wrap;\n  cursor: pointer;\n}\n.input{\n  background-color: transparent;\n  border: none;\n  width: 80%;\n  margin-top: 10%;;\n  border-bottom: darkslateblue 1px solid;\n  outline: none;\n}\ninput, textarea {\n  background-color: #d1d1d1;\n}\n.inputRadioParent{\n  width: 80%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  height: 40px;\n  margin-right: 10px;\n}\n.inputRadio{\n  margin-top: 50px;\n  margin-left: 15px;\n  cursor: pointer;\n}\n\n::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */\n  color: black;\n  opacity: 0.8; /* Firefox */\n  font-size: 14px;\n}\n\n:-ms-input-placeholder { /* Internet Explorer 10-11 */\n  color: black;\n  font-size: 14px;\n}\n\n::-ms-input-placeholder { /* Microsoft Edge */\n  color: black;\n  font-size: 14px;\n}\n.password-eye{\n  cursor: pointer;\n  position: absolute;\n  bottom: 70%;\n  right: 15%;\n  font-size: 18px !important;\n}\n#btn-sign-in{\n  border: none;\n  width: 80%;\n  margin-top: 10%;\n  background-color: rgba(255,255,255,0.8);\n  outline: none;\n  border-radius: 40px;\n  height: 50px;\n}\n.login-btn{\n  color: cornflowerblue;\n  cursor: pointer;\n}\n.paragraph1{\n  color: white;\n  margin-top: 5%;\n  text-align: center;\n  font-size: 15px;\n}\n.dark{\n  font-size: 14px;\n  text-shadow: 1px 1px black !important;\n}\n</style>"]}]}